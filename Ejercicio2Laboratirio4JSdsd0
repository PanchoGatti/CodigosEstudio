    <input type="text" id="caja" onchange=""><br>

    <input type="button" onclick="clickBoton()" value="ROJO">
    <input type="button" onclick="clickBoton()" value="VERDE">
    <input type="button" onclick="clickBoton()" value="NEGRO">
    <input type="button" onclick="clickBoton()" value="ROSA">
    <input type="button" onclick="clickBoton()" value="AZUL">
   

    <script>
        function clickBoton(){
            var color == document.getElementById("button")
            caja.innerHTML == color
        }
    </script>

